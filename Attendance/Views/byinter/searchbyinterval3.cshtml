<script type="text/javascript">
    $(document).ready(function () {
        $("#frominputdate").datepicker({

            dateFormat: "yy-mm-dd"
        });
    });
</script>
<script type="text/javascript">
    $(document).ready(function () {
        $("#toinputdate").datepicker({

